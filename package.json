{
    "name": "vue-app",
    "version": "1.0.1",
    "private": true,
    "author": "wyw",
    "license": "ISC",
    "description": "vue-app-template",
    "keywords": [
        "vue3",
        "typescript",
        "element-plus",
        "vite",
        "rollup"
    ],
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-customizable"
        },
        "cz-customizable": {
            "config": "./.cz-config.cjs"
        }
    },
    "scripts": {
        "dev": "vite --host --open",
        "build": "vue-tsc --noEmit && vite build",
        "build-only": "vite build",
        "optimize": "vite optimize",
        "preview": "vite preview --host --open",
        "test": "vitest",
        "test-ui": "vitest --ui",
        "test-preview": "vite preview --outDir ./html",
        "prepare": "husky install",
        "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write src/",
        "commit": "git add . && git cz",
        "write-permission": "tsc --skipLibCheck ./script/get-permission/get-permission.ts && node ./script/get-permission/get-permission.js",
        "write-api": "tsc --skipLibCheck ./script/generate-api/get-api-docs.ts && node ./script/generate-api/get-api-docs.js",
        "generate-api": "cd ./script/generate-api && npx openapi-generator-cli generate -c openapitools.json --generator-key v2.0",
        "open-api": "cd ./script/open-api && npx openapi-generator-cli generate -c openapitools.json --generator-key v2.0",
        "sort": "eslint --fix --ext .js,.jsx,.ts,.tsx,vue ./src ",
        "generate-router": "tsc --skipLibCheck ./script/generate-router/generate-router.ts --outDir ./script/generate-router/js && node ./script/generate-router/js/generate-router.js"
    },
    "dependencies": {
        "@element-plus/icons-vue": "^2.3.1",
        "@vue-office/docx": "^1.6.2",
        "@vue-office/excel": "^1.7.11",
        "@vue-office/pdf": "^2.0.2",
        "@vueuse/core": "^10.11.1",
        "@wangeditor/editor": "^5.1.23",
        "@wangeditor/editor-for-vue": "^5.1.12",
        "animejs": "^3.2.2",
        "async-validator": "^4.2.5",
        "axios": "^1.6.8",
        "dat.gui": "^0.7.9",
        "dayjs": "^1.11.10",
        "docx-preview": "^0.3.0",
        "echarts": "^5.5.0",
        "element-plus": "^2.6.2",
        "exceljs": "^4.4.0",
        "file-saver": "^2.0.5",
        "fs-extra": "^11.2.0",
        "html2canvas": "^1.4.1",
        "jsbarcode": "^3.11.6",
        "jspdf": "^2.5.1",
        "lodash-es": "^4.17.21",
        "luckyexcel": "^1.0.1",
        "mitt": "^3.0.1",
        "multiparty": "^4.2.3",
        "nprogress": "^0.2.0",
        "ol": "^9.1.0",
        "p-limit": "^6.2.0",
        "pinia": "^2.1.7",
        "print-js": "^1.6.0",
        "qrcode.vue": "^3.4.1",
        "sortablejs": "^1.15.2",
        "store2": "^2.14.3",
        "three": "^0.162.0",
        "vue": "^3.4.21",
        "vue-i18n": "^9.10.2",
        "vue-router": "^4.3.0",
        "vuedraggable": "^4.1.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.2.1",
        "@commitlint/config-conventional": "^19.1.0",
        "@openapitools/openapi-generator-cli": "^2.13.1",
        "@tsconfig/node18": "^18.2.4",
        "@types/animejs": "^3.1.12",
        "@types/file-saver": "^2.0.7",
        "@types/jsdom": "^21.1.6",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^20.11.30",
        "@types/tunnel": "^0.0.7",
        "@typescript-eslint/eslint-plugin": "^7.4.0",
        "@typescript-eslint/parser": "^7.4.0",
        "@vitalets/google-translate-api": "9.2.0",
        "@vitejs/plugin-legacy": "^5.3.2",
        "@vitejs/plugin-vue": "^5.0.4",
        "@vitejs/plugin-vue-jsx": "^3.1.0",
        "@vitest/ui": "^1.4.0",
        "@vue/eslint-config-prettier": "^9.0.0",
        "@vue/eslint-config-typescript": "^13.0.0",
        "@vue/test-utils": "^2.4.5",
        "@vue/tsconfig": "^0.5.1",
        "cz-customizable": "^7.0.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-define-config": "^2.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "5.1.3",
        "eslint-plugin-simple-import-sort": "^12.0.0",
        "eslint-plugin-vue": "^9.24.0",
        "husky": "^9.0.11",
        "jsdom": "^24.0.0",
        "lint-staged": "^15.2.2",
        "pinyin-pro": "^3.26.0",
        "prettier": "^3.2.5",
        "rollup-plugin-visualizer": "^5.12.0",
        "sass": "^1.72.0",
        "terser": "^5.30.0",
        "tunnel": "^0.0.6",
        "typescript": "~5.4.3",
        "unplugin-auto-import": "^0.17.5",
        "unplugin-element-plus": "^0.8.0",
        "unplugin-icons": "^0.18.5",
        "unplugin-vue-components": "^0.26.0",
        "vite": "^5.2.6",
        "vite-plugin-compression": "^0.5.1",
        "vite-plugin-dts": "^3.8.1",
        "vite-plugin-inspect": "^0.8.3",
        "vite-plugin-vue-devtools": "7.3.9",
        "vitest": "^1.4.0",
        "vue-tsc": "^2.0.7"
    },
    "json-comments": {
        "tips": "请安装JSONComments插件为JSON文件提供注释功能, 详见: https://github.com/zhangfisher/json_comments_extension",
        "script/open-api/openapitools.json": {
            "generator-cli.generators.v2.0.additionalProperties.supportsES6": "是否支持 ES6 模块",
            "generator-cli.generators.v2.0.additionalProperties.withSeparateModelsAndApi": "是否将模型和 API 分开生成",
            "generator-cli.generators.v2.0.generatorName": "生成器的名称，决定了使用哪种语言和框架生成代码",
            "generator-cli.generators.v2.0.output": "生成代码的输出目录",
            "generator-cli.generators.v2.0.inputSpec": "OpenAPI 规范的输入文件路径",
            "generator-cli.generators.v2.0.templateDir": "自定义模板的目录，用于覆盖默认模板",
            "generator-cli.generators.v2.0.additionalProperties.apiPackage": "API 服务的包名",
            "generator-cli.generators.v2.0.additionalProperties.modelPackage": "模型类的包名",
            "generator-cli.generators.v2.0.additionalProperties.withInterfaces": "是否生成接口",
            "generator-cli.generators.v2.0.additionalProperties": "定义额外的属性，用于定制生成的代码",
            "generator-cli.generators": "定义使用的生成器及其配置",
            "generator-cli.generators.v2.0": "指定 OpenAPI 规范的版本",
            "$schema": "引用配置文件的架构，用于验证配置文件的正确性",
            "spaces": "定义代码缩进使用的空格数量",
            "generator-cli": "配置 OpenAPI Generator CLI 的设置",
            "generator-cli.version": "CLI 工具的版本号"
        }
    }
}
