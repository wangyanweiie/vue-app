        "@vue/tsconfig": "^0.4.0",
        "eslint": "^8.45.0",
        "eslint-plugin-vue": "^9.15.1",
        "husky": "^8.0.3",
        "jsdom": "^22.1.0",
        "lint-staged": "^13.2.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.0.0",
        "sass": "^1.66.1",
        "typescript": "~5.1.6",
