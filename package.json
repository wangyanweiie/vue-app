        "jsbarcode": "^3.11.5",
        "lodash-es": "^4.17.21",
        "mitt": "^3.0.1",
        "nprogress": "^0.2.0",
        "pinia": "^2.1.4",
        "print-js": "^1.6.0",
        "qrcode.vue": "^3.4.1",
        "store2": "^2.14.2",
        "vue": "^3.3.4",
        "vue-router": "^4.2.4",
        "vuedraggable": "^4.1.0"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.3.2",
        "@tsconfig/node18": "^18.2.0",
