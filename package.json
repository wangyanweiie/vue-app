    "dayjs": "^1.11.9",
    "echarts": "^5.4.3",
    "element-plus": "^2.3.9",
    "jsbarcode": "^3.11.5",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.4",
    "print-js": "^1.6.0",
    "qrcode.vue": "^3.4.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
